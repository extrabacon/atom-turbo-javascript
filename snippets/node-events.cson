".source.js":
  "event handler":
    prefix: "on"
    body: """
    ${1:emitter}.on('${2:event}', function (${3:arguments}) {
    \t${0}
    });
    """
  "chain event handler":
    prefix: ".on"
    body: ".on('${2:event}', function (${3:arguments}) {${0}})"

