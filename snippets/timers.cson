".source.js, .source.ts, .source.tsx, .source.js-semantic":
  "setTimeout":
    prefix: "st"
    body: """
    setTimeout(() => {
    \t${0}
    }, ${1:delay});
    """
  "setInterval":
    prefix: "si"
    body: """
    setInterval(() => {
    \t${0}
    }, ${1:delay});
    """
  "setImmediate":
    prefix: "sim"
    body: """
    setImmediate(() => {
    \t${0}
    });
    """
  "process nextTick":
    prefix: "nt"
    body: """
    process.nextTick(() => {
    \t${1:}
    });
    """
